{
  "name": "api",
  "version": "1.0.0",
  "main": "server.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:push": "npx drizzle-kit push",
    "dev:worker": "nodemon --watch src --ext ts,json --exec \"tsx ./src/queues/worker.ts\"",
    "index:qdrant": "nodemon src --ext ts,json --exec \"tsx ./src/db/qdrant-index.ts\"",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose src/__tests__/unit/*.test.ts",
    "test:integration": "vitest run --reporter=verbose src/__tests__/integration/*.test.ts",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@huggingface/inference": "^4.13.3",
    "@langchain/community": "^1.0.3",
    "@langchain/core": "^1.0.5",
    "@langchain/groq": "^1.0.1",
    "@langchain/qdrant": "^1.0.0",
    "@langchain/textsplitters": "^1.0.0",
    "@neondatabase/serverless": "^1.0.2",
    "@qdrant/js-client-rest": "^1.15.1",
    "better-auth": "^1.3.34",
    "bullmq": "^5.63.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "groq-sdk": "^0.35.0",
    "http-status": "^2.1.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "redis": "^5.9.0",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "@types/wav": "^1.0.4",
    "@vitest/coverage-v8": "^4.0.15",
    "drizzle-kit": "^0.31.6",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
